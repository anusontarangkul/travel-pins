{
  "name": "travel-pins",
  "version": "1.0.0",
  "description": "Creating a social media app that will inspire people to travel the world.",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anusontarangkul/travel-pins.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anusontarangkul/travel-pins/issues"
  },
  "homepage": "https://github.com/anusontarangkul/travel-pins#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "sequelize": "^6.3.5",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
